{
  "name": "tek-o-thon",
  "version": "1.0.0",
  "description": "React app for Tek-O-thon hackathon",
  "license": "MIT",
  "homepage": "https://tekothon.in",
  "keywords": [
    "react",
    "reactjs",
    "Tek-O-thon",
    "Hackathon",
    "tek-o-thon"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/anton": "^4.5.9",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.13",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.1",
    "bootstrap": "4.5.3",
    "classnames": "2.3.1",
    "moment": "2.29.4",
    "perfect-scrollbar": "1.5.5",
    "react": ">=16.8.0",
    "react-countdown-circle-timer": "^3.0.9",
    "react-dom": ">=16.8.0",
    "react-image-viewer-dv": "^1.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "reactstrap": "8.7.1",
    "sass": "1.54.0",
    "web-vitals": "2.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "optionalDependencies": {
    "jquery": "3.6.0",
    "typescript": "4.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "sass src/assets/scss/blk-design-system-react.scss src/assets/css/blk-design-system-react.css",
    "minify-sass": "sass src/assets/scss/blk-design-system-react.scss src/assets/css/blk-design-system-react.min.css --style compressed",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  }
}
